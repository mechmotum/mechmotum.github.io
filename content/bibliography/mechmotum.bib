@inproceedings{Moore2019,
  title = {Expanded {{Optimization}} for {{Discovering Optimal Lateral Handling Bicycles}}},
  booktitle = {Bicycle and {{Motorcycle Dynamics}}: {{Symposium}} for {{Dynamics}} and {{Control}} of {{Single Track Vehicles}}},
