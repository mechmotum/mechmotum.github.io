@article{Schwab2013,
  title = {Rider Control Identification in Bicycling Using Lateral Force Perturbation Tests},
  author = {Schwab, A. L. and {de Lange}, P. D. L. and Happee, R. and Moore, Jason K.},
  year = {2013},
