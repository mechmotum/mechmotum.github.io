@article{Reijne2018,
  title = {Measuring and {{Comparing Descend}} in {{Elite Race Cycling}} with a {{Perspective}} on {{Real-Time Feedback}} for {{Improving Individual Performance}}},
  author = {Reijne, M. M. and Bregman, D. J. J. and Schwab, A. L.},
  year = {2018},
